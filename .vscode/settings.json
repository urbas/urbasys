{
  "python.defaultInterpreterPath": "${workspaceFolder}/.direnv/devEnv/bin/python",
  "[python]": {
    "editor.tabSize": 4
  },
  "python.testing.pytestArgs": [],
  "python.testing.pytestPath": "${workspaceFolder}/.direnv/devEnv/bin/python",
  "python.testing.unittestEnabled": false,
  "python.testing.pytestEnabled": true,
  "python.linting.enabled": true,
  "python.linting.pylintEnabled": false,
  "python.linting.flake8Enabled": true,
  "python.linting.flake8Path": "${workspaceFolder}/.direnv/devEnv/bin/flake8",
  "python.formatting.provider": "black",
  "python.formatting.blackPath": "${workspaceFolder}/.direnv/devEnv/bin/black",
  "files.exclude": {
    "**/__pycache__": true,
    "**/*.egg-info": true,
    "**/.venv": true,
    "states/urbasys/python/**/*": true
  },
  "files.watcherExclude": {
    "**/__pycache__": true,
    "**/*.egg-info": true,
    "**/.venv": true,
    "states/urbasys/python/**/*": true
  }
}